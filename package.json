{
  "name": "gemstone-front",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "homepage": "https://wesleygurgel.github.io/gemstone-front/",
  "scripts": {
    "dev": "vite --mode localhost",
    "dev:development": "vite --mode development",
    "dev:production": "vite --mode production",
    "build": "tsc && vite build",
    "build:development": "tsc && vite build --mode development",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:development": "vite preview --mode development",
    "preview:production": "vite preview --mode production",
    "predeploy": "npm run build:production",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
